<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.todor.tooblars"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.todor.toolbars.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.todor.toolbars.activity.ExpandableToolbarActivity"/>

        <activity
            android:name="com.todor.toolbars.activity.ParallaxToolbarActivity"/>

        <activity
            android:name="com.todor.toolbars.activity.HideToolbarActivity"/>

        <activity
            android:name="com.todor.toolbars.activity.ToolbarWithTabLayoutActivity"/>

        <activity android:name="com.todor.toolbars.activity.NotificationsActivity"/>

    </application>

</manifest>
